#include <bits/stdc++.h>
using namespace std;

int vis[10001];

int main() {
	cin.tie(0)->sync_with_stdio(0);
	int cnt = 0, n, m; cin >> m >> n;
	for (int i = 0; i < n; i++) {
		int l, r; cin >> l >> r;
		for (int j = l; j <= r; j++) {
			if (!vis[j]++) cnt++;
		}
	}
	cout << (cnt == m ? "YES" : "NO");
}