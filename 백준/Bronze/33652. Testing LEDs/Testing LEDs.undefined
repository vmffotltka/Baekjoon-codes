#include <bits/stdc++.h>
using namespace std;

int main() {
    cin.tie(0)->sync_with_stdio(0);
    int n; cin >> n;
    long long ans = LLONG_MAX;
    for (int i = 0; i < n; i++) {
        int a, b; cin >> a >> b;
        if (b == 0)
            ans = min(ans, (long long)a);
    }
    cout << (ans == LLONG_MAX ? -1 : ans);
}