#include <bits/stdc++.h>
using namespace std;

int cnt[26];

int main() {
    cin.tie(0)->sync_with_stdio(0);
    string s; cin >> s;
    for (auto c : s) {
        cnt[c - 'A']++;
    }
    int ans = 0;
    for (int i = 0; i < 26; i++) {
        if (cnt[i])
            ans += max(1, cnt[i] - 1);
    }
    cout << ans;
}