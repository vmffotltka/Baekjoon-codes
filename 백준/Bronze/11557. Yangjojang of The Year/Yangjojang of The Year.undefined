#include <bits/stdc++.h>
using namespace std;

int main() {
    cin.tie(0)->sync_with_stdio(0);
    int t; cin >> t; while (t--) {
        int n; cin >> n;
        string name; int mx = -1;
        for (int i = 0; i < n; i++) {
            string s; int p; cin >> s >> p;
            if (p > mx) {
                mx = p;
                name = s;
            }
        }
        cout << name << "\n";
    }
}