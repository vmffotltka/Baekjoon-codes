#include <bits/stdc++.h>
using namespace std;

int main() {
    cin.tie(0)->sync_with_stdio(0);
    int n, m, y, x, k;
    cin >> n >> m >> y >> x >> k;
    x--, y--;
    int ans = 0;
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < m; j++) {
            int dist = abs(i - y) + abs(j - x);
            if (dist <= k && !(i == y && j == x)) {
                ans++;
            }
        }
    }
    cout << ans;
}