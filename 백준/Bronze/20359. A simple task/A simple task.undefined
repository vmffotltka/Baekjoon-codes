#include <bits/stdc++.h>
using namespace std;

int main() {
	cin.tie(0)->sync_with_stdio(0);
	int n; cin >> n;
	for (int i = 1; i <= n; i += 2) {
		if (n % i != 0) continue;
		int cur = n / i, cnt = 0;
		bool tri = true;
		while (cur != 1) {
			if (cur & 1) { tri = false; break; }
			cur >>= 1, cnt++;
		}
		if (!tri) continue;
		cout << i << " " << cnt;
		return 0;
	}
}