#include <bits/stdc++.h>
using namespace std;

int arr[100];

int main() {
    cin.tie(0)->sync_with_stdio(0);
    int n, m; cin >> n >> m;
    for (int i = 0; i < n; i++) cin >> arr[i];
    
    long long ans = 0;
    for (int i = 0; i < m; i++) {
        int p; cin >> p;
        for (int j = 0; j < n; j++) {
            ans += (arr[j] + p) * max(arr[j], p);
        }
    }
    cout << ans;
}