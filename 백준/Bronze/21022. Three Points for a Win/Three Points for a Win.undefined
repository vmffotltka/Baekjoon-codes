#include <bits/stdc++.h>
using namespace std;

int arr[100];

int main() {
	cin.tie(0)->sync_with_stdio(0);
	int n, ans = 0; cin >> n;
	for (int i = 0; i < n; i++) cin >> arr[i];
	for (int i = 0; i < n; i++) {
		int p; cin >> p;
		if (p < arr[i]) ans += 3;
		else if (p == arr[i]) ans++;
	}
	cout << ans;
}