#include <bits/stdc++.h>
using namespace std;

int vis[101];

int main() {
	cin.tie(0)->sync_with_stdio(0);
	int n, m, k; cin >> n >> m >> k;
	for (int i = 0; i < m + k; i++) {
		int p; cin >> p;
		vis[p] = 1;
	}
	cout << n - m - k << '\n';
	for (int i = 1; i <= n; i++) {
		if (vis[i]) continue;
		cout << i << " ";
	}
}