#include <bits/stdc++.h>
using namespace std;

int main() {
    cin.tie(0)->sync_with_stdio(0);
    int n; cin >> n;
    int ans = 0, song = 1;
    while (n > 0) {
        if (n - song < 0) {
            song = 1;
        }
        n -= song;
        ans++, song++;
    }
    cout << ans;
}