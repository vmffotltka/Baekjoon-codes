#include <bits/stdc++.h>
using namespace std;

int main() {
    cin.tie(0)->sync_with_stdio(0);
    string s; cin >> s;
    int l = (s.length() - 1) / 2, r = s.length() / 2;
    while (l >= 0 && r < (int)s.length() && s[l] == s[r]) {
        l--; r++;
    }
    cout << (l == -1 ? "beep" : "boop");
}