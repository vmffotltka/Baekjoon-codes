#include <bits/stdc++.h>
using namespace std;

int main() {
    cin.tie(0)->sync_with_stdio(0);
    string a, b; cin >> a >> b;
    for (int i = 0; i < b.length(); i++) {
        bool found = false;
        for (int j = 0; j < a.length(); j++) {
            if (b[i] == a[j]) {
                found = true;
                break;
            }
        }
        if (!found) cout << b[i];
    }
}