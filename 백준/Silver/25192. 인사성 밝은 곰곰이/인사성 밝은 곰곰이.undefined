#include <bits/stdc++.h>
using namespace std;

int main() {
	cin.tie(0)->sync_with_stdio(0);
	int n, ans = 0; cin >> n;
	map<string, int> m;
	for (int i = 0; i < n; i++) {
		string s; cin >> s;
		if (s == "ENTER") { ans += m.size(); m.clear(); }
		else m[s]++;
	}
	cout << ans + m.size();
}