#include <bits/stdc++.h>
using namespace std;

int main() {
	cin.tie(0)->sync_with_stdio(0);
	int n, cur = 1; cin >> n;
	stack<int> st;
	for (int i = 0; i < n; i++) {
		int p; cin >> p;
		st.push(p);
		if (cur == p) {
			while (!st.empty() && st.top() == cur) {
				st.pop();
				cur++;
			}
		}
	}
	while (!st.empty()) {
		if (st.top() != cur) {
			cout << "Sad";
			return 0;
		}
		st.pop();
		cur++;
	}
	cout << "Nice";
}