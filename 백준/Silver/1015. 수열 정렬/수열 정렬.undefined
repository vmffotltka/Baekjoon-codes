#include <bits/stdc++.h>
using namespace std;

pair<int, int> arr[50];
int ans[50];

int main() {
    cin.tie(0)->sync_with_stdio(0);
    int n; cin >> n;
    for (int i = 0; i < n; i++) {
        cin >> arr[i].first;
        arr[i].second = i;
    }
    sort(arr, arr + n);
    for (int i = 0; i < n; i++) {
        ans[arr[i].second] = i;
    }
    for (int i = 0; i < n; i++) {
        cout << ans[i] << " ";
    }
}