#include <bits/stdc++.h>
using namespace std;

int arr[20], dp[10001];

int main() {
	cin.tie(0)->sync_with_stdio(0);
	int t; cin >> t; while (t--) {
		int n; cin >> n;
		for (int i = 0; i < n; i++) {
			cin >> arr[i];
		}
		int m; cin >> m;
		memset(dp, 0, sizeof(dp));
		for (int i = 0; i < n; i++) {
			dp[arr[i]]++;
			for (int j = arr[i] + 1; j <= m; j++) {
				dp[j] += dp[j - arr[i]];
			}
		}
		cout << dp[m] << '\n';
	}
}