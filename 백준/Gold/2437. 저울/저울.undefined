#include <bits/stdc++.h>
using namespace std;

int N, arr[1000];

int main() {
	cin.tie(0)->sync_with_stdio(0);
	cin >> N;
	for (int i = 0; i < N; i++) cin >> arr[i];
	sort(arr, arr + N);
	int sum = 0;
	for (int i = 0; i < N; i++) {
		if (sum + 1 < arr[i] && arr[i] != 1) {
			cout << sum + 1;
			return 0;
		}
		sum += arr[i];
	}
	cout << sum + 1;
}